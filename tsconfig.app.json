{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "noEmit": true
  },
  "include": ["env.d.ts", "src/**/*", "src/**/*.ts", "src/**/*.vue", "vite-plugin-fallback.ts"],
  "exclude": ["src/**/__tests__/*"]
}
